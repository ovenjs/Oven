{
  "name": "ovenjs",
  "version": "0.1.0",
  "description": "A modern Discord API wrapper for Node.js",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "clean": "pnpm -r run clean",
    "test:run": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "discord-api-types": "^0.37.83"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "discord",
    "api",
    "wrapper",
    "bot",
    "typescript"
  ],
  "author": "OvenJS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ovenjs/oven.git"
  }
}
