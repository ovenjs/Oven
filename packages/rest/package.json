{
  "name": "@ovendjs/rest",
  "version": "1.0.0",
  "description": "Enhanced Discord REST API client with advanced features including intelligent rate limiting, request caching, batching, and comprehensive event handling.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ovendjs/utils": "^0.22.1",
    "@vladfrangu/async_event_emitter": "^2.4.6",
    "discord-api-types": "^0.37.83",
    "undici": "^7.12.0",
    "zod": "^4.0.13"
  },
  "keywords": [
    "discord",
    "api",
    "rest",
    "client",
    "rate-limit"
  ],
  "author": "OvenJS Team",
  "license": "MIT"
}
